@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@RenderBody()

@section Theme { energy-journey }

@section ShoppingBasketToggler {
    @RenderSection("ShoppingBasketToggler", false)
}

@section mainSectionTheme {
    @RenderSection("mainSectionTheme", required: false)
}

@section Popups {
