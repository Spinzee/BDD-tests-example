@model Dictionary<string, string>
@{
    @* see https://developers.google.com/tag-manager/devguide *@

    <script>
        dataLayer = [
            {
                @foreach (KeyValuePair<string, string> dataElement in Model)
                {
                    @Html.Raw($"'{dataElement.Key}':'{dataElement.Value}',")
                }
            }
        ]
    </script>
}