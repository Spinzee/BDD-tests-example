@using Products.WebModel.Resources.Common
@{
    ViewBag.Title = Resources.PageNotFoundTitle;
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1>@Resources.PageNotFoundHeader</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12 col-sm-11 col-md-8 gap">
            <p class="gap">@Resources.PageNotFoundParagraph</p>
            <a id="back-to-home-link" class="button button-primary" href="@System.Configuration.ConfigurationManager.AppSettings["LinkBackToHomeURL"]" title="@Resources.LinkBackToHomeAlt">@Resources.LinkBackToHome</a>
        </div>
    </div>
</div>